	<!-- openFormats -->

	<openFormatsImportWindow Caption='Перетворювач openFormats - $AppName$'>
		<!-- Label's -->
		<lbOfFileName Caption='Файл для перетворення:'/>
		<lbOfFiles Caption='Файли для перетворення:'/>
		<lbNewFileName Caption='Імʼя нового файлу:'/>
		<!-- Button's -->
		<btnAdd Caption='Додати'/>
		<btnRemove Caption='Видалити'/>
		<btnCancel Caption='Скасувати'/>
		<btnOk Caption='Далі'/>
		<tbLeanMore Caption='Дізнатися більше про openFormats'/>
