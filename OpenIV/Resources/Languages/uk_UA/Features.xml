	<!-- Archive Defragmenter -->

	<DefragmenterWindow Caption='Дефрагментація архівів - $AppName$'>
		<!-- Labels -->
		<lbDescription Caption="Інструмент дефрагментації перетворює дані всередині архіву на неперервну послідовність файлів для заощадження місця на жорсткому диску."/>
		<lbNoteSave Caption='Зроблені під час редагування зміни зберігаються незалежно від дефрагментації, тож не слід використовувати її після кожного редагування.'/>
		<lbNoteRevert Caption='Дефрагментація не здатна скасувати зроблені зміни або відновити оригінальний стан файлів.'/>
		<!-- Actions -->
		<actClose Caption='Закрити'/>
		<actRebuild Caption='Дефрагментувати'/>
		<actAddFile Caption='Додати файли'/>
		<actAddFolder Caption='Додати папку'/>
		<actRemoveFile Caption='Видалити файл'/>
		<actClear Caption='Очистити'/>
		<!-- Columns -->
		<lvFiles>
			<Columns>
				<Item Index='0' Caption=''/>
				<Item Index='1' Caption='Імʼя'/>
				<Item Index='2' Caption='Розташування'/>
				<Item Index='3' Caption='Тип'/>
			</Columns>
		</lvFiles>
		<!-- Messages -->
		<AeroWindow LinesCount='4'>
			<StringList>
				<Item Index='0' Value='Дефрагментацію зроблено.'/>
				<Item Index='1' Value='Дефрагментація архівів - $AppName$'/>
				<Item Index='2' Value='Відкрити дефрагментований архів?'/>
				<Item Index='3' Value='Деякі файли не дефрагментовано.'/>
			</StringList>
		</AeroWindow>
	</DefragmenterWindow>

	<!-- Archive properties -->

	<ArchivePropertiesWindow Caption='Інформація про архів'>
