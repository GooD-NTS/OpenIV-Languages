<?xml version="1.0" encoding="UTF-8"?>
<Translation>

	<!-- Favorites -->

	<AddToFavoritesWindow Caption='添加一個收藏'>
		<Label1 Caption='名稱：'/>
		<Label2 Caption='創建於：'/>

		<btnAdd Caption='添加'/>
		<btnCancel Caption='取消'/>
		<btnNewFolder Caption='新建文件夾'/>

		<AeroWindow LinesCount='3'>
			<StringList>
				<Item Index='0' Value='收藏'/>
				<Item Index='1' Value='新建一個文件夾'/>
				<Item Index='2' Value='文件夾名： '/>
			</StringList>
		</AeroWindow>
	</AddToFavoritesWindow>

	<OrganizeFavoritesWindow Caption='組織收藏 - $AppName$'>
		<btnSave Caption='保存'/>
		<btnCancel Caption='取消'/>
		<btnNewFolder Caption='新建文件夾'/>
		<btnRemove Caption='移除'/>
		<btnClear Caption='清空'/>

		<Label1 Caption='使用拖放來組織你的收藏。'/>

		<AeroWindow LinesCount='3'>
			<StringList>
				<Item Index='0' Value='收藏'/>
				<Item Index='1' Value='新建一個文件夾'/>
				<Item Index='2' Value='文件夾名： '/>
			</StringList>
		</AeroWindow>
	</OrganizeFavoritesWindow>

	<!-- Archive Defragmenter -->

	<DefragmenterWindow Caption='重建 - $AppName$'>
		<!-- Labels -->
		<lbDescription Caption="重新整理RPF文件內的文件碎片以優化文件體積"/>
		<lbNoteSave Caption='你所做的更改已自動保存, 每次編輯後並不用必需重建。'/>
		<lbNoteRevert Caption='重建不會改變文件內容'/>
		<!-- Actions -->
		<actClose Caption='關閉'/>
		<actRebuild Caption='重建'/>
		<actAddFile Caption='添加文件'/>
		<actAddFolder Caption='添加文件夾'/>
		<actRemoveFile Caption='移除文件'/>
		<actClear Caption='清空列表'/>
		<!-- Columns -->
		<lvFiles>
			<Columns>
				<Item Index='0' Caption=''/>
				<Item Index='1' Caption='名稱'/>
				<Item Index='2' Caption='位置'/>
				<Item Index='3' Caption='類型'/>
			</Columns>
		</lvFiles>
		<!-- Messages -->
		<AeroWindow LinesCount='4'>
			<StringList>
				<Item Index='0' Value='全部完成。'/>
				<Item Index='1' Value='$AppName$ - 文件已重建'/>
				<Item Index='2' Value='打開已重建的文件？'/>
				<Item Index='3' Value='部分文件未做處理。'/>
			</StringList>
		</AeroWindow>
	</DefragmenterWindow>

	<!-- Archive properties -->

	<ArchivePropertiesWindow Caption='RPF信息'>
		<!-- Button's -->
		<btnClose Caption='關閉'/>
		<!-- Label's -->
		<lbType Caption='類型：'/>
		<lbVersion Caption='版本：'/>
		<lbCount Caption='物件數：'/>

		<lbCGame Caption='當前遊戲：'/>
		<lbCPlatform Caption='當前平臺：'/>

		<lbCodeModule Caption='模塊：'/>
		<lbCMVersion Caption='模塊版本：'/>
	</ArchivePropertiesWindow>

	<!-- openFormats -->

	<openFormatsImportWindow Caption='$AppName$ - 導入openFormats文件'>
		<!-- Label's -->
		<lbOfFileName Caption='導入文件：'/>
		<lbOfFiles Caption='導入文件：'/>
		<lbNewFileName Caption='新文件名：'/>
		<!-- Button's -->
		<btnAdd Caption='添加'/>
		<btnRemove Caption='移除'/>
		<btnCancel Caption='取消'/>
		<btnOk Caption='確定'/>
		<tbLeanMore Caption='瞭解openFormats'/>
		<!-- Check box -->
		<cbProtectMesh Caption='保護網格（以防日後被導出）'/>
	</openFormatsImportWindow>

	<openFormatsValidatorWindow Caption='$AppName$ - openFormats驗證器'>
		<lbTitle Caption='openFormats導入報告'/>
		<btnClose Caption='關閉'/>
		<!-- Strings -->
		<AeroWindow LinesCount='100'>
			<StringList>
				<Item Index='0' Value='部分文件未導入，更多信息如下。'/>
				<!-- Version errors  -->
				<Item Index='1' Value='%S(Line %D): 不支持的資源版本， actual: "%S" but expected "%D"。'/>
				<Item Index='2' Value='%S(Line %D): 不支持的格式版本， actual: "%S" but expected "%D"。'/>
				<!-- Common errors -->
				<Item Index='3' Value='致命錯誤：無法初始化Direct3D。'/>
				<Item Index='4' Value='致命錯誤：文件"%S"未找到。'/>
				<Item Index='5' Value='%S(Line %D)：文件"%S"未找到。'/>
				<Item Index='6' Value='%S(Line %D)：文件損壞。'/>
				<Item Index='7' Value='%S(Line %D)：屬性"%s" expected 但"%s" found。'/>
				<Item Index='8' Value='%S(Line %D)：屬性"%s"未找到。'/>
				<Item Index='9' Value='%S(Line %D)：實例參數不足。'/>
				<!-- Collision errors -->
				<Item Index='10' Value='%S(Line %D)：未知碰撞限制類型"%S"。'/>
				<Item Index='11' Value='%S(Line %D)：碰撞過大。'/>
				<Item Index='12' Value='%S(Line %D)：碰撞損壞。'/>
				<!-- Skeleton errors -->
				<Item Index='13' Value='%S(Line %D)：骨點名為null。'/>
				<!-- Geometry/Graphics/Drawables errors -->
				<Item Index='14' Value='%S(Line %D)：網格被鎖定。'/>
				<Item Index='15' Value='%S(Line %D)：材料索引超出限制。'/>
				<Item Index='16' Value="%S(Line %D)：著色器無法被覆蓋。"/>		
				<Item Index='17' Value="%S(Line %D)：單個幾何體中骨點過多，你最多隻能使用48個骨點。"/>

				<Item Index='18' Value="%S(Line %D)：%S"/>						
			</StringList>
		</AeroWindow>	
	</openFormatsValidatorWindow>	

</Translation>
