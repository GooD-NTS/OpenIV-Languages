				<Item Index='17' Value="%S(Ligne %D): Trop d'os dans la géométrie, vous ne pouvez pas en utiliser plus de 48."/>

				<Item Index='18' Value="%S(Line %D): %S"/>

				<!-- Export/Import -->
				<Item Index='19' Value="Rapport d'importation openFormats"/>
