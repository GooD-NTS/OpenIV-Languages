<?xml version="1.0" encoding="UTF-8"?>
<Translation>

	<!-- Favorites -->

	<AddToFavoritesWindow Caption='Ajouter aux favoris'>
		<Label1 Caption='Nom :'/>
		<Label2 Caption='Créer dans :'/>

		<btnAdd Caption='Ajouter'/>
		<btnCancel Caption='Annuler'/>
		<btnNewFolder Caption='Nouveau dossier'/>

		<AeroWindow LinesCount='3'>
			<StringList>
				<Item Index='0' Value='Favoris'/>
				<Item Index='1' Value='Nouveau dossier'/>
				<Item Index='2' Value='Nom du dossier : '/>
			</StringList>
		</AeroWindow>
	</AddToFavoritesWindow>

	<OrganizeFavoritesWindow Caption='Organiser les favoris - $AppName$'>
		<btnSave Caption='Sauvegarder'/>
		<btnCancel Caption='Annuler'/>
		<btnNewFolder Caption='Nouveau dossier'/>
		<btnRemove Caption='Supprimer'/>
		<btnClear Caption='Nettoyer'/>

		<Label1 Caption='Utilisez le glisser-déposer pour organiser vos favoris'/>

		<AeroWindow LinesCount='3'>
			<StringList>
				<Item Index='0' Value='Favoris'/>
				<Item Index='1' Value='Nouveau dossier'/>
				<Item Index='2' Value='Nom du dossier :'/>
			</StringList>
		</AeroWindow>
	</OrganizeFavoritesWindow>

	<!-- Archive Defragmenter -->

	<DefragmenterWindow Caption="Défragmenteur d'archive - $AppName$">
		<!-- Labels -->
		<lbDescription Caption="Le défragmenteur d'archive consolide les fichiers à l'intérieur de l'archive pour réduire l'espace disque qu'elle occupe."/>
		<lbNoteSave Caption="Tous les changements sont déjà sauvegardés, il est inutile d'utiliser le défragmenteur après chaque modification."/>
		<lbNoteRevert Caption="Le défragmenteur n'annulera aucun changement effectué à l'intérieur de l'archive."/>
		<!-- Actions -->
		<actClose Caption='Fermer'/>
		<actRebuild Caption='Défragmenter'/>
		<actAddFile Caption='Ajouter des fichiers'/>
		<actAddFolder Caption='Ajouter un dossier'/>
		<actRemoveFile Caption='Supprimer le fichier'/>
		<actClear Caption='Nettoyer la liste'/>
		<!-- Columns -->
		<lvFiles>
			<Columns>
				<Item Index='0' Caption=''/>
				<Item Index='1' Caption='Nom'/>
				<Item Index='2' Caption='Emplacement'/>
				<Item Index='3' Caption='Type'/>
			</Columns>
		</lvFiles>
		<!-- Messages -->
		<AeroWindow LinesCount='4'>
			<StringList>
				<Item Index='0' Value='Tout est terminé.'/>
				<Item Index='1' Value="Défragmenteur d'archive - $AppName$"/>
				<Item Index='2' Value="Ouvrir l'archive défragmentée ?"/>
				<Item Index='3' Value="Certains fichiers n'ont pas été traités."/>
			</StringList>
		</AeroWindow>
	</DefragmenterWindow>

	<!-- Archive properties -->

	<ArchivePropertiesWindow Caption="Informations à propos de l'archive">
		<!-- Button's -->
		<btnClose Caption='Fermer'/>
		<!-- Label's -->
		<lbType Caption='Type :'/>
		<lbVersion Caption='Version :'/>
		<lbCount Caption='Nombre de fichiers :'/>

		<lbCGame Caption='Jeu actuel :'/>
		<lbCPlatform Caption='Plateforme actuelle :'/>

		<lbCodeModule Caption='Module :'/>
		<lbCMVersion Caption='Version du module :'/>
	</ArchivePropertiesWindow>

	<!-- openFormats -->

	<openFormatsImportWindow Caption='Importer des openFormats - $AppName$'>
		<!-- Label's -->
		<lbOfFileName Caption='Importer le fichier :'/>
		<lbOfFiles Caption='Importer les fichiers:'/>
		<lbNewFileName Caption='Nom du nouveau fichier :'/>
		<!-- Button's -->
		<btnAdd Caption='Ajouter'/>
		<btnRemove Caption='Supprimer'/>
		<btnCancel Caption='Annuler'/>
		<btnOk Caption='OK'/>
		<tbLeanMore Caption='En savoir plus à propos des openFormats'/>
		<!-- Check box -->
		<cbProtectMesh Caption='Protéger la Mesh (empêcher un futur export)'/>
	</openFormatsImportWindow>

	<openFormatsValidatorWindow Caption="Validateur d'openFormats - $AppName$">
		<btnClose Caption='Fermer'/>
		<!-- Strings -->
		<AeroWindow LinesCount='100'>
			<StringList>
				<Item Index='0' Value="Certains fichiers n'ont pas été importés, plus d'informations ci-après."/>
