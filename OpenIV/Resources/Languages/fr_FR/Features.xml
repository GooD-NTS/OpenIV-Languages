				<Item Index='1' Value='%S(Ligne %D): Version de ressource non supportée, (actuelle : « %S », « %D » requise.'/>
				<Item Index='2' Value='%S(Ligne %D): Version de ressource non supportée, (actuelle : « %S », « %D » requise.'/>
				<!-- Common errors -->
				<Item Index='3' Value="Erreur fatale : Impossible d'initialiser Direct3D."/>
				<Item Index='4' Value='Erreur fatale : Fichier « %S » introuvable.'/>
				<Item Index='5' Value='%S(Ligne %D): Fichier « %S » introuvable.'/>
				<Item Index='6' Value='%S(Ligne %D): Fichier corrompu.'/>
				<Item Index='7' Value='%S(Ligne %D): Propriété « %s » attendue mais « %s » trouvée.'/>
				<Item Index='8' Value='%S(Ligne %D): Propriété « %s » introuvable.'/>
				<Item Index='9' Value='%S(Ligne %D): Pas assez de paramètres.'/>
				<!-- Collision errors -->
				<Item Index='10' Value='%S(Ligne %D): Type de bord de collision inconnu « %S ».'/>
				<Item Index='11' Value='%S(Ligne %D): Collision trop importante.'/>
				<Item Index='12' Value='%S(Ligne %D): Collision corrompue.'/>
				<!-- Skeleton errors -->
				<Item Index='13' Value="%S(Line %D): Aucun nom d'os."/>
				<!-- Geometry/Graphics/Drawables errors -->
				<Item Index='14' Value='%S(Ligne %D): Mesh verrouillé.'/>
				<Item Index='15' Value='%S(Ligne %D): Indice de matériel hors-limite.'/>
